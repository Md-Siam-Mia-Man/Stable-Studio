{
  "applicationId": "js.neutralino.Stablegui",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "window": {
    "title": "Stable GUI",
    "width": 1280,
    "height": 800,
    "minWidth": 900,
    "minHeight": 650,
    "center": true,
    "resizable": true,
    "maximize": false,
    "icon": "/resources/assets/icons/icon.png",
    "enableInspector": true,
    "borderless": false,
    "alwaysOnTop": false
  },
  "cli": {
    "binaryName": "Stable-Studio-Installer",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "5.3.0",
    "clientVersion": "5.3.0"
  },
  "modes": {
    "window": {
      "title": "Stable GUI",
      "width": 1280,
      "height": 800,
      "minWidth": 900,
      "minHeight": 650,
      "center": true,
      "resizable": true,
      "icon": "/resources/assets/icons/icon.png",
      "enableInspector": true,
      "borderless": false,
      "alwaysOnTop": false
    }
  },
  "nativeAllowList": ["app.*", "os.*", "window.*", "filesystem.*", "debug.log"]
}
